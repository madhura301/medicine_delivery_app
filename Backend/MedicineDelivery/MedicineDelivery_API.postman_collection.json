					}
				},
				{
					"name": "Get Orders By Customer ID",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Response is an array\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.be.an('array');",
									"});",
									"",
									"if (pm.response.code === 200 && pm.response.json().length > 0) {",
									"    var orders = pm.response.json();",
									"    pm.collectionVariables.set(\"orderId\", orders[0].orderId);",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{token}}"
							}
						],
						"url": {
							"raw": "{{baseUrl}}/api/orders/customer/{{customerId}}",
							"host": ["{{baseUrl}}"],
							"path": ["api", "orders", "customer", "{{customerId}}"]
						},
						"description": "Get all orders belonging to a specific customer. Example customerId: 00000000-0000-0000-0000-000000000001"
					},
					"response": []
				},
				{
					"name": "Get Active Orders By Medical Store ID",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Response is an array\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.be.an('array');",
									"});",
									"",
									"pm.test(\"All orders are not Completed\", function () {",
									"    var orders = pm.response.json();",
									"    orders.forEach(function(order) {",
									"        pm.expect(order.orderStatus).to.not.equal('Completed');",
									"    });",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{token}}"
							}
						],
						"url": {
							"raw": "{{baseUrl}}/api/orders/medicalstore/{{medicalStoreId}}/active",
							"host": ["{{baseUrl}}"],
							"path": ["api", "orders", "medicalstore", "{{medicalStoreId}}", "active"]
						},
						"description": "Get all active orders (status != Completed) for a medical store. Example medicalStoreId: 00000000-0000-0000-0000-000000000001"
					},
					"response": []
				},
